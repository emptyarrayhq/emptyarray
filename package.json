{
  "name": "satellite",
  "version": "0.0.1",
  "description": "building a second brain for next generation makers",
  "main": "index.js",
  "repository": "https://github.com/marchhq/satellite.git",
  "author": "march-dev",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.9",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "@typescript-eslint/eslint-plugin": "6.4.0",
      "@typescript-eslint/parser": "6.4.0"
    }
  }
}

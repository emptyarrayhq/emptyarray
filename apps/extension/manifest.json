{
    "manifest_version": 3,
    "name": "march",
    "description": "An extension for march",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "storage",
      "contextMenus"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup/popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
    //   "16": "icons/icon16.png",
    //   "48": "icons/icon48.png",
    //   "128": "icons/icon128.png"
    }
  }
  